// Benchmark 2: Matrix Multiplication
// â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

fn matmul(N) {
    let A = matrix(N, N, 1.5)~
    let B = matrix(N, N, 2.0)~
    let C = matrix(N, N, 0.0)~
    
    seal for i in 0..N {
        seal for j in 0..N {
            let sum = 0.0~
            seal for k in 0..N {
                sum = sum + A[i][k] * B[k][j]~
            }
            C[i][j] = sum~
        }
    }
    
    return C[0][0]~
}

print("\n[2/5] Matrix Multiplication Benchmark")~
print("    Computing 64x64 matrix multiply...")~

start = time()~
let mat_result = matmul(64)~
let mat_time = time() - start~

print("    Checksum:", mat_result)~
print("    Time:", mat_time, "Î¼s")~
print("    âœ“ Complete")~

